<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="1260.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.ihmc.humanoidBehaviors.ui.behaviors.PatrolBehaviorUIController">
    <children>
        <GridPane hgap="15.0" vgap="2.0" layoutX="15.0" layoutY="17.0">
            <children>
                <Button fx:id="homeAll" onAction="#homeAll" prefWidth="130.0" text="Home All" />
                <Button fx:id="freeze" onAction="#freeze" prefWidth="130.0" text="Freeze" GridPane.rowIndex="1" />
                <Button fx:id="standPrep" onAction="#standPrep" prefWidth="130.0" text="Stand Prep" GridPane.rowIndex="2" />

                <Button fx:id="continuePatrol" onAction="#continuePatrol" prefWidth="130.0" text="Continue Patrol" GridPane.rowIndex="3" />
                <Button fx:id="pausePatrol" onAction="#pausePatrol" prefWidth="130.0" text="Pause Patrol" GridPane.rowIndex="4" />
                <Button fx:id="destroyPatrol" onAction="#destroyPatrol" prefWidth="130.0" text="Destroy Patrol" GridPane.rowIndex="5" />

                <Button onAction="#clearFlat" text="Clear Flat" GridPane.rowIndex="6"/>

                <Button fx:id="placeWaypointA" onAction="#placeWaypointA" prefWidth="130.0" text="Place Waypoint A" GridPane.rowIndex="7" />
                <Button fx:id="placeWaypointB" onAction="#placeWaypointB" prefWidth="130.0" text="Place Waypoint B" GridPane.rowIndex="8" />
            </children>
        </GridPane>
    </children>
</AnchorPane>