#ifndef __controller_msgs__msg__TaskspaceTrajectoryStatusMessage__idl__
#define __controller_msgs__msg__TaskspaceTrajectoryStatusMessage__idl__

#include "geometry_msgs/msg/./Point_.idl"
#include "geometry_msgs/msg/./Quaternion_.idl"
module controller_msgs
{
  module msg
  {
    module dds
    {
      const octet TRAJECTORY_EXECUTION_STATUS_STARTED =
      0;

      const octet TRAJECTORY_EXECUTION_STATUS_COMPLETED =
      1;


      /**
       * This message is part of the IHMC whole-body controller API.
       * The walking controller uses this message to report when it completed the execution of a trajectory message.
       */
      @TypeCode(type="controller_msgs::msg::dds_::TaskspaceTrajectoryStatusMessage_")
      struct TaskspaceTrajectoryStatusMessage
      {
        /**
         * Unique ID of the trajectory message this status refers to.
         */
        unsigned long sequence_id;
        /**
         * Refers to the name of the rigid-body that executed the trajectory.
         */
        string body_name;
        /**
         * The current status for the trajectory execution.
         */
        @defaultValue(value=255)
        octet trajectory_status;
        /**
         * The desired position (in world coordinates) of the rigid-body sent to the controller.
         */
        geometry_msgs::msg::dds::Point desired_body_position;
        /**
         * The desired orientation (in world coordinates) of the rigid-body sent to the controller.
         */
        geometry_msgs::msg::dds::Quaternion desired_foot_orientation;
        /**
         * The position (in world coordinates) of where the rigid-body actually arrived when the trajectory finished.
         */
        geometry_msgs::msg::dds::Point actual_body_position;
        /**
         * The orientation (in world coordinates) of where the rigid-body actually arrived when the trajectory finished.
         */
        geometry_msgs::msg::dds::Quaternion actual_body_orientation;
      };
    };
  };
};

#endif
