<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.ihmc.footstepPlanning.ui.components.FootstepPlannerCostsUIController">
   <children>
       <GridPane hgap="15.0" layoutX="15.0" layoutY="17.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="150.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="20.0" prefWidth="150.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Yaw weight" GridPane.rowIndex="2" />
            <Label text="Cost per step" GridPane.rowIndex="3" />
            <Label text="Heuristics weight" GridPane.rowIndex="4" />

            <Spinner fx:id="yawWeight"  editable="true" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Spinner fx:id="costPerStep"  editable="true" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Spinner fx:id="heuristicsWeight"  editable="true" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
