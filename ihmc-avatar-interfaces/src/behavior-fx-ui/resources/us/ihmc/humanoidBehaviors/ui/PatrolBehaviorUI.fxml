<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="1260.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.ihmc.humanoidBehaviors.ui.behaviors.PatrolBehaviorUIController">
    <children>
        <GridPane hgap="15.0" vgap="2.0" layoutX="15.0" layoutY="17.0">
            <children>
                <Button fx:id="placeWaypoints" onAction="#placeWaypoints" prefWidth="130.0" text="Place Waypoints" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Button fx:id="goToWaypoint" onAction="#goToWaypoint" prefWidth="130.0" text="Go To Waypoint" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Spinner fx:id="waypointIndex" initialValue="0" editable="true" amountToStepBy="1" min="0" max="500" prefWidth="90.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                <Button fx:id="pauseWalking" onAction="#pauseWalking" prefWidth="130.0" text="Pause Walking" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            </children>
        </GridPane>
    </children>
</AnchorPane>