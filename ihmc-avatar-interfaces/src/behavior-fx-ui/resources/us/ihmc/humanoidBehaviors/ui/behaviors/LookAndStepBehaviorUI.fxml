<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="187.0" prefWidth="861.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <FlowPane prefWrapLength="1300.0">
         <children>
            <VBox alignment="CENTER">
               <children>
                  <CheckBox fx:id="enablePlanningCheckBox" mnemonicParsing="false" onAction="#enablePlanningCheckBox" text="Enable planning">
                     <padding>
                        <Insets bottom="5.0" />
                     </padding>
                  </CheckBox>
                      <Button fx:id="takeStep" mnemonicParsing="false" onAction="#takeStep" text="Take Step" />
                  <TextField fx:id="behaviorState" />
               </children>
               <FlowPane.margin>
                  <Insets />
               </FlowPane.margin>
            </VBox>
            <TableView fx:id="lookAndStepParameterTable" prefHeight="210.0" prefWidth="290.0">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </TableView>
            <VBox>
               <children>
                  <Button fx:id="saveLookAndStepParameters" mnemonicParsing="false" onAction="#saveLookAndStepParameters" text="Save">
                     <VBox.margin>
                        <Insets bottom="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <TableView fx:id="footstepPlannerParameterTable" prefHeight="210.0" prefWidth="475.0">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </TableView>
            <VBox>
               <children>
                  <Button fx:id="saveFootstepPlanningParameters" mnemonicParsing="false" onAction="#saveFootstepPlanningParameters" text="Save">
                     <VBox.margin>
                        <Insets bottom="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children></FlowPane>
    </children>
</AnchorPane>
