#ifndef __controller_msgs__msg__ChestHybridJointspaceTaskspaceTrajectoryMessage__idl__
#define __controller_msgs__msg__ChestHybridJointspaceTaskspaceTrajectoryMessage__idl__

#include "controller_msgs/msg/./JointspaceTrajectoryMessage_.idl"
#include "controller_msgs/msg/./SO3TrajectoryMessage_.idl"
#include "std_msgs/msg/./Header_.idl"
module controller_msgs
{
  module msg
  {
    module dds
    {

      /**
       * This message is part of the IHMC whole-body controller API.
       * This message commands the controller to move the chest in both taskspace and jointspace
       * to the desired orientation and joint angles while going through the specified trajectory points.
       */
      @TypeCode(type="controller_msgs::msg::dds_::ChestHybridJointspaceTaskspaceTrajectoryMessage_")
      struct ChestHybridJointspaceTaskspaceTrajectoryMessage
      {
        /**
         * As of March 2018, the header for this message is only use for its sequence ID.
         */
        std_msgs::msg::dds::Header header;
        /**
         * The taskspace trajectory information.
         */
        controller_msgs::msg::dds::SO3TrajectoryMessage taskspace_trajectory_message;
        /**
         * The jointspace trajectory information.
         * The indexing for the joints goes increasingly from the joint the closest to the pelvis to the joint the closest to the chest.
         */
        controller_msgs::msg::dds::JointspaceTrajectoryMessage jointspace_trajectory_message;
      };
    };
  };
};

#endif
