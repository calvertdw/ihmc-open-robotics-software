      @TypeCode(type="controller_msgs::msg::dds_::QuadrupedTeleopDesiredVelocity_")
      struct QuadrupedTeleopDesiredVelocity
      {
        /**
         * Unique ID used to identify this message, should preferably be consecutively increasing.
