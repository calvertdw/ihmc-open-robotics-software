<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="1260.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="us.ihmc.humanoidBehaviors.ui.behaviors.StepInPlaceBehaviorUIController">
    <children>
        <GridPane hgap="15.0" vgap="2.0" layoutX="15.0" layoutY="17.0">
            <children>
                <Button fx:id="startStepping" onAction="#startStepping" prefWidth="130.0" text="Start Stepping" GridPane.rowIndex="1" />
                <Button fx:id="pauseStepping" onAction="#pauseStepping" prefWidth="130.0" text="Pause Stepping" GridPane.rowIndex="2" />
            </children>
        </GridPane>
    </children>
</AnchorPane>